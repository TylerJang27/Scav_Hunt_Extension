{
    "name": "Scavenger Hunt",
    "version": "0.9",
    "manifest_version": 2,
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "browser_action": {
      "default_title": "Scavenger Hunt"
    },
    "background": {
      "scripts": ["background.js", "encrypt.js"]
    },
    "permissions": ["tabs"],
    "web_accessible_resources": [
        "res/*"
    ]
  }